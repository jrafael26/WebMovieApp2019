@model IEnumerable<WebApplicationApp.Models.MembershipType>



<h1 class="page-header">Membership Types!!!</h1>
<p>
    <a href="/MembershipTypes/Create" class="btn btn-primary btn-xs pull-right"><b>+</b> Add new membership type</a>
</p>
<table class="table table-bordered table-hover">

    <tr>
        <th>Membership Type</th>
        <th>SignUp Fee</th>
        <th>Duration In Months</th>
        <th>Discount Rate</th>
        <th class="text-right">Action</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "Edit", new { id = item.Id })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SignUpFee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DurationInMonths)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiscountRate)
            </td>
            <td class="text-right">
                @Html.ActionLink(" ", "Edit", new { id = item.Id }, new { @class = "glyphicon glyphicon-edit btn btn-info btn-xs" })
                @Html.ActionLink(" ", "Details", new { id = item.Id }, new { @class = "glyphicon glyphicon-list btn btn-success btn-xs" })
                @Html.ActionLink(" ", "Delete", new { id = item.Id }, new { @class = "glyphicon glyphicon-remove btn btn-danger btn-xs" })
            </td>
        </tr>
    }

</table>
